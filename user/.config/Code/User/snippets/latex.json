  "Label": {
  "prefix": "lab",
  "body": [
    "\\label{${1|eq,fig,tab,sec,chap,thm,def|}:${2:label}}"
  ],
  "description": "Label"
  },
    
  "BBMrows": {
  "prefix": "bbmrows",
  "body": [
    "\\bbm", 
    "  ${1}",
    "\\ebm"
  ],
  "description": "BBM multi rows"
  },

  "BBM": {
  "prefix": "bbm",
  "body": [
    "\\bbm ${1} \\ebm"
  ],
  "description": "BBM"
  },
  "%%": {
  "prefix": "%%",
  "body": [
    "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
  ],
  "description": "Line comment"
  },
  "%=": {
    "prefix": "%=",
    "body": [
    "%==============================================================================="
    ],
    "description": "Line comment"
  },
  "%-": {
    "prefix": "%-",
    "body": [
    "%-------------------------------------------------------------------------------"
    ],
    "description": "Line comment"
  },

  "Expnential": {
  "prefix": "myexp",
  "body": [
    "\\exp\\left(${1}\\right)"
  ],
  "description": "BBM"
  },

  "Packed enumerate": {
    "prefix": "penum",
    "body": [
    "\\begin{packed_enum}",
    "  \\item ${1}",
    "\\end{packed_enum}"
    ],
    "description": "Packed enumeration"
  },

  "Packed itemize": {
    "prefix": "pitem",
    "body": [
    "\\begin{packed_itemize}",
    "  \\item ${1}",
    "\\end{packed_itemize}"
    ],
    "description": "Packed itemize"
  },

  "Equation break": {
    "prefix": "eqnbrk",
    "body": [
    "  \\nonumber \\\\\\\\",
    "&\\mathrel{\\phantom{=}} \\negmedspace {} ${1}"    
    ],
    "description": "Equation break"
  },
  
}