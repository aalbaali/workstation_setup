{
  // Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
  // same ids are connected.
  // Example:
  // "Print to console": {
  //   "prefix": "log",
  //   "body": [
  //   "console.log('$1');",
  //   "$2"
  //   ],
  //   "description": "Log output to console"
  // }
  // 
  // Checkout the snippet generator: https://snippet-generator.app/
  // 
  "Generate figure": {
  "prefix": "BFIG",
  "body": [
    "\\begin{figure}[${1:Hhtbp}]",
    "  \\centering",
    "  \\includegraphics[width=${2:0.8\\textwidth}]{$3}  ",
    "  \\caption{${4:caption}}",
    "  \\label{fig:${5:label}}",
    "\\end{figure}"
  ],
  "description": "Generate figure"
  },
  
  "Generate subfigure": {
  "prefix": "BSFIG",
  "body": [
    "\\begin{figure}[${1:Hhtbp}]",
    "  \\centering",
    "  \\begin{subfigure}{${2:0.45\\textwidth}}",
    "  \\includegraphics[width=\\textwidth]{$3}",
    "  \\caption{${4:subfig_1_caption>}}",
    "  \\label{fig:${5:subfig_1_label}}",
    "  \\end{subfigure}",
    "  %",
    "  \\begin{subfigure}{${2:0.45\\textwidth}}",
    "  \\includegraphics[width=\\textwidth]{$6}",
    "  \\caption{${7:subfig_2_caption}}",
    "  \\label{fig:${8:subfig_2_label}}",
    "  \\end{subfigure}",
    "  \\caption{${9:fig_caption}}",
    "  \\label{fig:${10:fig_label}}",
    "\\end{figure}"
  ],
  "description": "Generate subfigure"
  },
  
  "Generate columns": {
  "prefix": "bcols",
  "body": [
    "\\begin{columns}",
    "  \\begin{column}[c]{${1:0.5\\textwidth}}",
    "  ${3} ",
    "  \\end{column}",
    "  \\begin{column}[c]{${2:0.5\\textwidth}}",
    "  ",
    "  \\end{column}",
    "\\end{columns}",
  ],
  "description": "Presentation columns"
  },
  

  "Section": {
  "prefix": "sec",
  "body": [
    "\\section{${1}}",
    "\\label{sec:${1}}"    
  ],
  "description": "section"
  },
  "Subsection": {
  "prefix": "ssec",
  "body": [
    "\\subsection{${1}}",
    "\\label{sec:${1}}"    
  ],
  "description": "subsection"
  },
  "Sububsection": {
  "prefix": "sssec",
  "body": [
    "\\subsubsection{${1}}",
    "\\label{sec:${1}}"    
  ],
  "description": "subsection"
  },

  "My align": {
  "prefix": "mali",
  "body": [
    "\\begin{align}",
    "  ${1}",
    "\\end{align}"
  ],
  "description": "My align"
  },

  "Box definition": {
  "prefix": "bdef",
  "body": [
    "\\begin{definitionBox}[${1:Title}]",
    "  ${2}",
    "\\end{definitionBox}"
  ],
  "description": "Definition box"
  },


  "Remark box": {
  "prefix": "bremark",
  "body": [
    "\\begin{remarkBox}",
    "\t${1}",
    "\\end{remarkBox}"
  ],
  "description": "Remark in a black box"
  },

  "Proposition box": {
  "prefix": "bproposition",
  "body": [
    "\\begin{propositionBox}",
    "\t${1}",
    "\\end{propositionBox}"
  ],
  "description": "Proposition in a black box"
  },
  
  "Bluebox": {
  "prefix": "blueBox",
  "body": [
    "\\begin{blueBox}",
    "\t${1}",
    "\\end{blueBox}"
  ],
  "description": "Blue box"
  },

  "Blackbox": {
  "prefix": "blackBox",
  "body": [
    "\\begin{blackBox}",
    "\t${1}",
    "\\end{blackBox}"
  ],
  "description": "Black box"
  },

  "Greykbox": {
  "prefix": "greykBox",
  "body": [
    "\\begin{greyBox}",
    "\t${1}",
    "\\end{greyBox}"
  ],
  "description": "Grey box"
  },

  "Theorem box": {
  "prefix": "bthm",
  "body": [
    "\\begin{theoremBox}",
    "   [${1:Title}]  ",
    "   ${2}",
    "\\end{theoremBox}"
  ],
  "description": "My theorem"
  },

  "Tufte definition": {
  "prefix": "tdef",
  "body": [
    "\\begin{tuftedefinition}[${1:Title}]",
    "  ${2}",
    "\\end{tuftedefinition}"
  ],
  "description": "My definition"
  },

  "Tufte theorem": {
  "prefix": "tthm",
  "body": [
    "\\begin{tuftetheorem}",
